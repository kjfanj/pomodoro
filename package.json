  "dependencies": {
    "body-parser": "^1.18.3",
    "concurrently": "^4.1.0",
    "cookie-session": "^1.3.3",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "mongoose": "^5.5.2"
  },
  "devDependencies": {
    "nodemon": "^1.18.11"
  }
}
